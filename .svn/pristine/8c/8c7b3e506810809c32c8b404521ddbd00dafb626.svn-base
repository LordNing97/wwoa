import org.springframework.stereotype.Component;

/**
 * @Author leisurexi
 * @Description
 * @Date 2019/8/31
 * @Time 16:32
 */
@Data
@ToString
@Component
public class FileConfig {

    @Value("${file.imgUrlPrefix}")
