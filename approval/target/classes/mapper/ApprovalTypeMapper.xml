<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xy.wwoa.approval.mapper.ApprovalTypeMapper">
    <!--  查询全部审批类型  -->
    <select id="findAll" resultType="com.xy.wwoa.approval.model.ApprovalType">
        select * from wwoa_approval_type
    </select>

    <select id="findById" resultType="com.xy.wwoa.approval.model.ApprovalType">
        select * from wwoa_approval_type where id = #{id}
    </select>
</mapper>